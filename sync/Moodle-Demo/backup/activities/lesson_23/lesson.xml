            <answer_text>&lt;p&gt;1. 192.168.8.1&lt;/p&gt;&lt;p&gt;2. 192.168.7.21&lt;br&gt;&lt;/p&gt;</answer_text>
            <response>&lt;p&gt;Ok!&lt;br&gt;&lt;/p&gt;</response>
            <answerformat>1</answerformat>
            <responseformat>1</responseformat>
            <attempts>
